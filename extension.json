{
	"name": "JSONer",
	"version": "0.0.1",
	"author": [
		"Jonas Gr√∂ger <jonas.groeger@noris.de>"
	],
	"url": "https://gitlab.noris.net/cda-ad/JSONer",
	"descriptionmsg": "A MediaWiki Extension to embed external JSON data into a page.",
	"license-name": "Proprietary",
	"type": "other",
	"AutoloadClasses": {
		"JSONerHooks": "JSONer.hooks.php",
		"SpecialHelloWorld": "specials/SpecialHelloWorld.php"
	},
	"config": {
		"JSONerEnableFoo": true
	},
	"ExtensionMessagesFiles": {
		"JSONerAlias": "JSONer.i18n.alias.php"
	},
	"Hooks": {
		"NameOfHook": [
			"JSONerHooks::onNameOfHook"
		]
	},
	"MessagesDirs": {
		"JSONer": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.jsoner.foo": {
			"scripts": [
				"modules/ext.jsoner.js",
				"modules/ext.jsoner.foo.js"
			],
			"styles": [
				"modules/ext.jsoner.foo.css"
			],
			"messages": [],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "JSONer"
	},
	"SpecialPages": {
		"HelloWorld": "SpecialHelloWorld"
	},
	"manifest_version": 1
}
