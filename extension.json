	"version": "0.0.1",
	"author": [
      "Jonas Gr√∂ger ([mailto:jonas.groeger@noris.de Mail], [https://jonasgroeger.de Web])"
	],
	"url": "https://gitlab.noris.net/cda-ad/JSONer",
	"descriptionmsg": "jsoner-desc",
	"type": "parserhook",
	"config": {
		"JSONerBaseUrl": "https://example.com/rest",
		"JSONerUser": "invalid.user",
		"JSONerPass": "invalid.pass"
	},
	"AutoloadClasses": {
		"JSONerHooks": "JSONer.hooks.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"JSONerHooks::onParserSetup"
		]
	},
	"ExtensionMessagesFiles": {
		"JSONerMagic": "JSONer.i18n.magic.php"
	},
	"MessagesDirs": {
		"JSONer": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.jsoner.foo": {
			"scripts": [
				"modules/ext.jsoner.js",
				"modules/ext.jsoner.foo.js"
			],
			"styles": [
				"modules/ext.jsoner.foo.css"
			],
			"messages": [],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"remoteExtPath": "JSONer"
	},
	"manifest_version": 1
}
